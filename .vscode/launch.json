{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./build/SaponseEx.elf",
            "request": "launch",
            "preLaunchTask": "Build project",
            "type": "cortex-debug",
            "servertype": "jlink",
            "armToolchainPath": "C:\\tools\\gcc-arm-none-eabi-10.3-2021.10\\bin\\",
            "serverpath": "C:\\Program Files\\SEGGER\\JLink\\JLinkGDBServerCL.exe",
            "device": "STM32F412CG",
            "interface": "swd",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 100000000,
                "swoFrequency": 12000000,
                "source": "probe",
                "decoders": [
                    {
                        "label": "SWO output",
                        "type": "console",
                        "port": 0,
                        "showOnStartup": true,
                        "encoding": "ascii"
                    },
                    {
                        "label": "graph output",
                        "type": "binary",
                        "port": 1,
                        "showOnStartup": true,
                        "encoding": "unsigned"
                    },
                    {
                        "type": "graph",
                        "port": 1,
                        "encoding": "unsigned",
                        "scale": 1,
                        "graphId": "1"
                    }
                ]
            },
            "graphConfig": [
                {
                    "label": "Graph",
                    "timespan": 30,
                    "type": "realtime",
                    "annotate": false,
                    "maximum": 256,
                    "minimum": 0,
                    "plots": [
                        {
                            "graphId": "1",
                            "label": "adcvalue",
                            "color": "#53753c"
                        }
                    ]
                }
            ]
        }
    ]
}